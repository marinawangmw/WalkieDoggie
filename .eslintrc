{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-console":0,
    "jsx-a11y/no-autofocus":0
  },
  "plugins": ["react", "import", "prettier", "react-native"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "import/ignore": [
    "node_modules/react-native/index\\.js$",
    ["react-native"]
  ], 
  "import/resolver": {
    "node": {
      "extensions": [".js", ".jsx", ".native.js"]
    }
  },
  "react": {
    "version": "detect"
  }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": false,
    "jest": true
  }
}
